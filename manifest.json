{
  "manifest_version": 2,
  "name": "Cloudinsight Mini",
  "version": "0.4.1",
  "description": "View your dashboard chart from Cloudinsight.",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "lib/background.js"
    ]
  },
  "permissions": [
    "https://*.oneapm.com/",
    "https://*.cloudinsight.io/",
    "http://www.google-analytics.com/",
    "http://www.googletagmanager.com/",
    "alarms",
    "storage"
  ],
  "browser_action": {
    "default_title": "Cloudinsight Mini",
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "author": "wyvernnot",
  "homepage_url": "https://github.com/cloudinsight/mini-chrome-extension"
}
