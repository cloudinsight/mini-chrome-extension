{
  "manifest_version": 2,
  "name": "Cloudinsight Mini",
  "version": "0.1.0",
  "description": "View your dashboard chart from Cloudinsight.",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-3.0.0.min.js",
      "bg.js"
    ]
  },
  "permissions": [
    "https://*.oneapm.com/",
    "https://*.cloudinsight.io/"
  ],
  "browser_action": {
    "default_title": "Cloudinsight Mini",
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "author": "wyvernnot",
  "homepage_url": "https://github.com/cloudinsight/mini-chrome-extension"
}
